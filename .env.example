# Google OAuth
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id

# Apple OAuth
REACT_APP_APPLE_CLIENT_ID=your_apple_client_id
REACT_APP_APPLE_REDIRECT_URI=https://your-app.com/auth/apple/callback

# OAuth Endpoints (if needed)
REACT_APP_GOOGLE_AUTH_ENDPOINT=https://accounts.google.com/o/oauth2/v2/auth
REACT_APP_APPLE_AUTH_ENDPOINT=https://appleid.apple.com/auth/authorize

# Scopes
REACT_APP_GOOGLE_SCOPES=email profile openid
REACT_APP_APPLE_SCOPES=name email
